<?xml version="1.0" encoding="UTF-8"?>
<ssh-config>
  <global-settings>
    <default-timeout>30</default-timeout>
    <compression>yes</compression>
  </global-settings>
  
  <host>
    <name>example-server</name>
    <hostname>example.com</hostname>
    <port>22</port>
    <user>username</user>
    <identity-file>~/.ssh/id_rsa</identity-file>
  </host>
  
  <host>
    <name>dev-server</name>
    <hostname>dev.example.com</hostname>
    <port>2222</port>
    <user>devuser</user>
    <identity-file>~/.ssh/dev_rsa</identity-file>
    <forward-agent>yes</forward-agent>
  </host>
</ssh-config>